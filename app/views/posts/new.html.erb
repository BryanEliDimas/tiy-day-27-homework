<!-- New Post page / Admin Page -->
<div class="row">
  <div class="col-xs-12 col-md-6 col-md-offset-3">

    <%= simple_form_for @post do |f| %>
      <%= f.input :title, as: :text %>
      <%= f.input :photo %>
      <%= f.input :tag_list, label: "Tags" %>
      <%= f.submit class: "btn btn-primary" %>
      <button class="btn btn-warning" style="float: right"> <%= link_to "Log out", sign_out_path, method: :delete %> </button>
    <% end %>

  </div>
</div>
